                                               BaseInfo 3.2.2
***********************************************************************************************************************************************/

(function () {
    var BaseInfoMain = function () {
        function BaseInfoCreate()
            {
                try
                    {
						qx.Class.define("BaseInfoLang", {
							type: "singleton",
							extend: qx.core.Object,
							construct: function (language) {
								/*
									Enthaltene Sprachen:
									de=deutsch (Welt) STANDART,
									en=englisch (World), ro=rumnisch (Lume), hu=ungarisch (Vilg),
									it=italienisch (Mondo), tr=trkisch (Dnya), fr=franzsisch (Monde), 
									
									Kommende Sprachen:
									Mundo = Spanisch, Portugiesisch, Brasilianisch
									Svt = tschechisch
									Svet = Slowakisch
								*/
								this.Languages = ['de','en','ro','hu','it','tr','fr'];
								if (language != null) {
									this.MyLanguage = language;
								}
							},
							members: {
								MyLanguage: "de",
								Languages: null,
								Data: null,

								loadData: function (language) {
									var l = this.Languages.indexOf(language);

									if (l < 0) {
										this.Data = null;
										return;
									}

									this.Data = new Object();
									// this.Data[DESCRIPTION] = [VALUES,VLAUES,...][l];
